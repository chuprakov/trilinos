LIBS = $(SUPLIB) $(IOSS) $(EXODUS) $(NEMESIS) $(NETCDF)  
CXXEXTRA_DEFINES = -I$(INCDIR)/ioss -DNO_MPI 

PROGS=exomatlab

SRCS = exomatlab.C EML_SystemInterface.C 
OBJS = $(SRCS:.C=.o)

NormalCCTarget($(PROGS), $(OBJS), NullParameter, NullParameter, $(LIBS))
InstallTarget(install,$(PROGS), $(INSTBINFLAGS),$(BINDIR))
DependTarget()



