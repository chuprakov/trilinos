# Note: all relative paths are relative to package/doc!
#
@INCLUDE = @CMAKE_CURRENT_SOURCE_DIR@/Doxyfile.options
TAGFILES += \
  $(TRILINOS_HOME)/packages/common/tag_files/teuchos.tag=$(TRILINOS_HOME)/packages/teuchos/doc/html \
  $(TRILINOS_HOME)/packages/common/tag_files/epetra.tag=$(TRILINOS_HOME)/packages/epetra/doc/html \
  $(TRILINOS_HOME)/packages/common/tag_files/epetra.tag=$(TRILINOS_HOME)/packages/tpetra/doc/html \
  $(TRILINOS_HOME)/packages/common/tag_files/muelu-cthulhu.tag=$(TRILINOS_HOME)/MueLu/muelu/cthulhu/doc/html
#
# Package options
#
PROJECT_NAME           = "MueLu"
OUTPUT_DIRECTORY       = .
#
# What and how documentation is extracted
#
INPUT                  = @CMAKE_CURRENT_SOURCE_DIR@/../src @CMAKE_CURRENT_SOURCE_DIR@/../gallery @CMAKE_CURRENT_SOURCE_DIR@/MueLu_DoxygenDocumentation.hpp
#FILE_PATTERNS          = *.h *c *.hpp *.cpp
RECURSIVE              = YES
EXCLUDE                = 
EXCLUDE_PATTERNS       = *.x *.o *.out
EXAMPLE_PATTERNS       =
EXAMPLE_PATH           = ../examples
IMAGE_PATH             = 
INPUT_FILTER           = 
ALPHABETICAL_INDEX     = YES
IGNORE_PREFIX          = 
EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = YES
#
# Links to other packages
#
ALLEXTERNALS           = NO
EXTERNAL_GROUPS        = NO
GENERATE_TAGFILE       = $(TRILINOS_HOME)/packages/common/tag_files/muelu.tag
