<ParameterList name="MueLu">
   
  <ParameterList name="Hierarchy">
    
    <Parameter name="numDesiredLevel" type="int" value="10"/>
    
    <ParameterList name="Levels">
      <ParameterList name="AllLevels">
        <Parameter name="startLevel" type="int" value="0"/>
        
        <ParameterList name="P">
          <Parameter name="type" type="string" value="TentativePFactory"/>
        </ParameterList>

        <ParameterList name="Smoother">
          <Parameter name="type" type="string" value="TrilinosSmoother"/>

          <ParameterList name="ParameterList">
            <Parameter name="type"                       type="string" value="RELAXATION"/>    
            <Parameter name="relaxation: type"           type="string" value="Symmetric Gauss-Seidel"/>
            <Parameter name="relaxation: sweeps"         type="int"    value="1"/>
            <Parameter name="relaxation: damping factor" type="int"    value="1"/>
          </ParameterList>

        </ParameterList>
      </ParameterList>

      </ParameterList>
      
      <ParameterList name="CoarsestLevel">
        <Parameter name="startLevel" type="int" value="-1"/>
        <Parameter name="type" type="string" value="DirectSolver"/>
      </ParameterList>
  </ParameterList>
      
</ParameterList>
