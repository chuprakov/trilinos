/* Ifpack2_config.h.in is preprocessed by CMake to create Ifpack2_config.h. */

/* Define if we want to build Ifpack2 with debug support */
#cmakedefine HAVE_IFPACK2_DEBUG

/* Define if we have MPI */
#cmakedefine HAVE_IFPACK2_MPI

/* Define if we have the QD extended-precision TPL */
#cmakedefine HAVE_IFPACK2_QD

/* Define if we have Zoltan2 */
#cmakedefine HAVE_IFPACK2_ZOLTAN2

/* Define if we have Xpetra */
#cmakedefine HAVE_IFPACK2_XPETRA

/* Define if explicit instantiation is enabled. */
#cmakedefine HAVE_IFPACK2_EXPLICIT_INSTANTIATION

/* Define if we have Amesos2 */
#cmakedefine HAVE_IFPACK2_AMESOS2

/* Define if building with the Boost library */
#cmakedefine HAVE_IFPACK2_BOOST

/* Define if building with the Cholmod library */
#cmakedefine HAVE_IFPACK2_CHOLMOD

/* Define if we have experimental. */
#cmakedefine HAVE_IFPACK2_EXPERIMENTAL

/* Define if we have support graph. */
#cmakedefine HAVE_IFPACK2_SUPPORTGRAPH

