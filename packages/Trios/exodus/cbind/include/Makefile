INCLIST=exodusII.h exodusII_int.h exodusII_ext.h

install::
	@for j in $(INCLIST) ; \
	do \
		echo "installing $$j in "$(INCDIR) ; \
		$(RM) $(INCDIR)/$$j ; \
		$(INSTALL) -c $(INSTINCFLAGS) $$j $(INCDIR)/$$j ; \
	done

