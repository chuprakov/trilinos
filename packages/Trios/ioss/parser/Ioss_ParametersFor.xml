<!--

   ELEMENT COORDINATE SYSTEM IS { RECTANGULAR | CYLINDRICAL | SPHERICAL }
-->
    <line_command usage="DBIO" physics="STRUCTURAL" instance="28" min_occurs="0" max_occurs="unbounded">
      <keyword>ELEMENT COORDINATE SYSTEM</keyword>
      <parameter name="Delimiter" type="ENUMERATED" enumeration="Delimiter" count="1"/>
      <parameter name="Rectangular|cylindrical|spherical" type="ENUMERATED" enumeration="rectangular|cylindrical|spherical" count="1"/>
      <summary>
        Provides a model name for the coordinate system specification:
        (1) xyz
        (2) cylindrical
        (3) spherical
      </summary>
    </line_command>
<!--

    POINT AX = ax
-->
    <line_command usage="DBIO" physics="STRUCTURAL" instance="29" min_occurs="0" max_occurs="unbounded">
      <keyword>POINT AX</keyword>
      <parameter name="Delimiter" type="ENUMERATED" enumeration="Delimiter" count="1"/>
      <parameter name="Ax" type="REAL" count="1"/>
      <summary>
       Global x-component of vector defining coordinate system direction A.
       It is not necessary to input A as a unit vector.
      </summary>
    </line_command>
<!--
    POINT AY = ay
-->
    <line_command usage="DBIO" physics="STRUCTURAL" instance="30" min_occurs="0" max_occurs="unbounded">
      <keyword>POINT AY</keyword>
      <parameter name="Delimiter" type="ENUMERATED" enumeration="Delimiter" count="1"/>
      <parameter name="Ay" type="REAL" count="1"/>
      <summary>
       Global y-component of vector defining coordinate system direction A.
       It is not necessary to input A as a unit vector.
      </summary>
    </line_command>
<!--
    POINT AZ = az
-->
    <line_command usage="DBIO" physics="STRUCTURAL" instance="31" min_occurs="0" max_occurs="unbounded">
      <keyword>POINT AZ</keyword>
      <parameter name="Delimiter" type="ENUMERATED" enumeration="Delimiter" count="1"/>
      <parameter name="Az" type="REAL" count="1"/>
      <summary>
       Global z-component of vector defining coordinate system direction A.
       It is not necessary to input A as a unit vector.
      </summary>
    </line_command>
<!--

   POINT BX = bx
-->
    <line_command usage="DBIO" physics="STRUCTURAL" instance="32" min_occurs="0" max_occurs="unbounded">
      <keyword>POINT BX</keyword>
      <parameter name="Delimiter" type="ENUMERATED" enumeration="Delimiter" count="1"/>
      <parameter name="Bx" type="REAL" count="1"/>
      <summary>
       Nominally the global x-component of vector defining coord system direction B.
       However, it is only necessary that the supplied B vector lie in
       the A-B plane, because C is constructed as (A x B) and B is redefined to
       be (C x A).  It is not necessary to input B as a unit vector.
      </summary>
    </line_command>
<!--
    POINT BY = by
-->
    <line_command usage="DBIO" physics="STRUCTURAL" instance="33" min_occurs="0" max_occurs="unbounded">
      <keyword>POINT BY</keyword>
      <parameter name="Delimiter" type="ENUMERATED" enumeration="Delimiter" count="1"/>
      <parameter name="By" type="REAL" count="1"/>
      <summary>
       Nominally the global y-component of vector defining coord system direction B.
       However, it is only necessary that the supplied B vector lie in
       the A-B plane, because C is constructed as (A x B) and B is redefined to
       be (C x A).  It is not necessary to input B as a unit vector.
      </summary>
    </line_command>
<!--
    POINT BZ = bz
