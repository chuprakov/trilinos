<!--
    HOURGLASS [ STIFFNESS | VISCOSITY ] = hgval
-->
    <line_command usage="DBIO" physics="STRUCTURAL" instance="4" min_occurs="0" max_occurs="unbounded">
      <keyword>HOURGLASS</keyword>
      <parameter name="Stiffness|Viscosity" type="ENUMERATED" enumeration="Stiffness|Viscosity" count="1"/>
      <parameter name="Delimiter" type="ENUMERATED" enumeration="Delimiter" count="1"/>
      <parameter name="Hgval" type="REAL" count="1"><nonnegative/></parameter>
      <summary>
        Supplies the hourglass parameters for this element block.
      </summary>
    </line_command>
<!--
    MEMBRANE HOURGLASS [STIFFNESS | VISCOSITY ] = mhgval
-->
    <line_command usage="DBIO" physics="STRUCTURAL" instance="40" min_occurs="0" max_occurs="unbounded">
      <keyword>MEMBRANE HOURGLASS</keyword>
      <parameter name="Stiffness|Viscosity" type="ENUMERATED" enumeration="Stiffness|Viscosity" count="1"/>
      <parameter name="Delimiter" type="ENUMERATED" enumeration="Delimiter" count="1"/>
      <parameter name="Hgval" type="REAL" count="1"><nonnegative/></parameter>
      <summary>
        Supplies the hourglass parameters for membrane deformation
        in a shell or membrane element block.
      </summary>
    </line_command>

<!--
    BENDING HOURGLASS [STIFFNESS | VISCOSITY ] = bhgval
