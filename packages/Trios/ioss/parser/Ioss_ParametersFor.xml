<!--
    LINEAR BULK VISCOSITY = lbv
-->
    <line_command usage="DBIO" physics="STRUCTURAL" instance="6" min_occurs="0" max_occurs="unbounded">
      <keyword>LINEAR BULK VISCOSITY</keyword>
      <parameter name="Delimiter" type="ENUMERATED" enumeration="Delimiter" count="1"/>
      <parameter name="Lbv" type="REAL" count="1"/>
      <summary>
        Supplies the linear coefficient for the bulk viscosity computations.
      </summary>
    </line_command>
<!--
    QUADRATIC BULK VISCOSITY = qbv
-->
    <line_command usage="DBIO" physics="STRUCTURAL" instance="7" min_occurs="0" max_occurs="unbounded">
      <keyword>QUADRATIC BULK VISCOSITY</keyword>
      <parameter name="Delimiter" type="ENUMERATED" enumeration="Delimiter" count="1"/>
      <parameter name="Qbv" type="REAL" count="1"/>
      <summary>
        Supplies the quadratic coefficient for the bulk viscosity computations.
      </summary>
    </line_command>
<!--
    SOLID MECHANICS USE MODEL modelname
-->
    <line_command usage="DBIO" physics="STRUCTURAL" instance="8" min_occurs="0" max_occurs="unbounded">
      <keyword>SOLID MECHANICS USE MODEL</keyword>
      <parameter name="Modelname" type="STRING" count="1"/>
      <summary>
        Associates a solid mechanics material model with this element block.
        The material parameters for this block are specified in the 
        material denoted by the MATERIAL command.
      </summary>
    </line_command>
<!--
    DEPOSIT SPECIFIC INTERNAL ENERGY edep OVER TIME tdep STARTING AT TIME tinit
-->
    <line_command usage="DBIO" physics="STRUCTURAL" instance="10" min_occurs="0" max_occurs="unbounded">
      <keyword>DEPOSIT SPECIFIC INTERNAL ENERGY</keyword>
      <parameter name="Edep" type="REAL" count="1"><positive/></parameter>
      <optional>
        <parameter name="OVER" type="TOKEN" token="OVER" count="1"/>
        <parameter name="TIME" type="TOKEN" token="TIME" count="1"/>
        <parameter name="Tdep" type="REAL" count="1"><nonnegative/></parameter>
        <parameter name="STARTING" type="TOKEN" token="STARTING" count="1"/>
        <parameter name="AT" type="TOKEN" token="AT" count="1"/>
        <parameter name="TIME" type="TOKEN" token="TIME" count="1"/>
        <parameter name="Tinit" type="REAL" count="1"><nonnegative/></parameter>
      </optional>
      <summary>
        Defines the amount of specific (per unit mass) internal energy
        to be deposited in the material. The energy is deposited over
        time tdep, beginning at time tinit. The optional parameters
        tdep and tinit both default to zero, so the energy will be
        deposited instantaneously at time zero if they are not specified.
        The deposition is uniform in space, so each element in the block
        has the same amount edep added to its specific internal energy. 
      </summary>
    </line_command>
<!--
    ELEMENT NUMERICAL FORMULATION IS { OLD | NEW }
-->
    <line_command usage="DBIO" physics="STRUCTURAL" instance="11" min_occurs="0" max_occurs="unbounded">
      <keyword>ELEMENT NUMERICAL FORMULATION</keyword>
      <parameter name="Delimiter" type="ENUMERATED" enumeration="Delimiter" count="1"/>
      <parameter name="Old|New" type="ENUMERATED" enumeration="Old|New" count="1"/>
      <summary>
        Specifies which element numerical formulation to use for this
        block.
      </summary>
    </line_command>
<!--
    EFFECTIVE MODULI MODEL = [elastic|current|presto|pronto]
-->
    <line_command usage="DBIO" physics="STRUCTURAL" instance="50" min_occurs="0" max_occurs="unbounded">
      <keyword>EFFECTIVE MODULI MODEL</keyword>
      <parameter name="Delimiter" type="ENUMERATED" enumeration="Delimiter" count="1"/>
      <parameter name="Elastic|current|presto|pronto" type="ENUMERATED" enumeration="elastic|current|presto|pronto" count="1"><nonnegative/></parameter>
      <summary>
        Specifies the method used to determine the effective moduli. This
        choice can have a significant effect on the resulting hourglassing
        behavior.  The models are:
           elastic: use the initial elastic moduli 
           current: use the tangent moduli without correction for edge
                    conditions (e.g. very small stiffnesses, softening
                    materials)
           presto:  use the PRESTO version of the pronto routine, which
                    includes some fairly clear errors. This is included 
                    only for backward compatibility with old PRESTO runs.
           pronto:  use the old PRONTO method for computing elastic moduli
                    this approach is straight out of PRONTO, PRESTO's
                    predecessor.
      </summary>
    </line_command>
<!--
    MAX ENERGY ITERATIONS = mei
-->
    <line_command usage="DBIO" physics="STRUCTURAL" instance="60" min_occurs="0" max_occurs="unbounded">
      <keyword>MAX ENERGY ITERATIONS</keyword>
      <parameter name="Delimiter" type="ENUMERATED" enumeration="Delimiter" count="1"/>
      <parameter name="Mei" type="INTEGER" count="1"><positive/></parameter>
      <summary>
        Specifies the maximum number of iterations to take in solving
        the implicit internal energy update equation. Applicable when
        using EOS material models with extracted energy updates.
      </summary>
    </line_command>
<!--
    ENERGY ITERATION TOLERANCE = eit
-->
    <line_command usage="DBIO" physics="STRUCTURAL" instance="61" min_occurs="0" max_occurs="unbounded">
      <keyword>ENERGY ITERATION TOLERANCE</keyword>
      <parameter name="Delimiter" type="ENUMERATED" enumeration="Delimiter" count="1"/>
      <parameter name="Eit" type="REAL" count="1"><positive/></parameter>
      <summary>
        Specifies the tolerance criteria for exiting the iterative
        solve of the implicit internal energy update equation.
        Applicable when using EOS material models with extracted
        energy updates.
      </summary>
    </line_command>
<!--
    DEACTIVATE {mechanics}
-->
    <line_command usage="DBIO" physics="GENERIC" instance="100" min_occurs="0" max_occurs="unbounded">
      <keyword>DEACTIVATE</keyword>
      <parameter name="CodeName" type="STRING" count="1"/>
      <summary>
        Deactivate this elemenent block for this mechanics.
      </summary>
    </line_command>
  </block_command><!-- END PARAMETERS FOR BLOCK blockname -->
  
<!--
    BEGIN PARAMETERS FOR PHASE phase_name
-->
  <block_command usage="DBIO" physics="LEVELSET" instance="1" min_occurs="0" max_occurs="unbounded">
    <keyword>PARAMETERS FOR PHASE</keyword>
    <line_parameter name="Phase Name"/>
    <summary>
      Specifies analysis parameters associated with each phase.
    </summary>
<!--
    WHERE {level set name} IS {negative|positive}
-->
    <line_command usage="DBIO" physics="LEVELSET" instance="10" min_occurs="0" max_occurs="unbounded">
      <keyword>WHERE</keyword>
      <parameter name="LevelSetName" type="STRING" count="1"/>
      <parameter name="Delimiter" type="ENUMERATED" enumeration="Delimiter" count="1"/>
      <parameter name="Negative|positive" type="ENUMERATED" enumeration="negative|positive" count="1"/>
      <summary>
        Specifies where this phase is defined with respect to the given level set field.
      </summary>
    </line_command>
  </block_command><!-- END PARAMETERS FOR PHASE blockname -->
</sierraCommandSyntax>
