#ifndef KOKKOS_LINALG_CONFIG_H
#define KOKKOS_LINALG_CONFIG_H

/*
 * "Optimization level" for KokkosLinAlg computational kernels.  The
 * higher the level, the more code variants get generated, and thus
 * the longer the compile times.  However, more code variants mean
 * both better performance overall, and more uniform performance for
 * corner cases.
 */
#define KOKKOSLINALG_OPT_LEVEL @KokkosLinAlg_Opt_Level@

#endif
