#!/bin/sh
../Trilinos/configure \
--with-mpi-compilers="/usr/local/mpich-1.2.7_gcc3.4.6/bin" \
--prefix=${PWD} \
--cache-file=config.cache \
--with-gnumake \
--disable-default-packages \
--enable-examples \
--enable-tests \
--enable-amesos \
--enable-amesos-klu \
--enable-aztecoo \
--enable-epetra \
--enable-kokkos \
--enable-epetraext \
--enable-galeri \
--enable-ifpack \
--enable-ml \
--enable-teuchos \
--enable-ifpack-ml \
--with-libs="/home/ikarlin/OSKI/install-debug/lib/oski/liboski.a" \
--with-incdirs="-I/home/ikarlin/OSKI/install-debug/include" \
--with-oski
